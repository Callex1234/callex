                                              <% }); %>
                                          </select>
                                          <div style="display: flex;">
                                            <button type="button" onclick="move_right(document.getElementById('availableTrunkAdd'),document.getElementById('selectedTrunkAdd'))" class="addbluebtn">
                                              <i
                                                class="fa-solid fa-forward"
