            <!-- Section Two End -->

            <!-- Footer -->
            <%- include('../layout/footer') %>
            <!-- Footer End -->
    
    </div>

    <script>
            
            function move_right(left, right) {
			for (var i = 0; i < left.options.length; i++) {
				if (left.options[i].selected) {
					right.options[right.options.length] = new Option(left.options[i].text, left.options[i].value);
					left.options[i--] = null;
				}
			}
		}

		function move_left(right, left) {
			for (var i = 0; i < right.options.length; i++) {
				if (right.options[i].selected) {
					left.options[left.options.length] = new Option(right.options[i].text, right.options[i].value);
					right.options[i--] = null;
				}
			}
		}

    function move_bottom(selected_tabs) {
			var selected_tabs = document.getElementById(selected_tabs);
			var selectOptions = selected_tabs.getElementsByTagName('option');
			for (var i = selectOptions.length - 2; i >= 0; i--) {
				var opt = selectOptions[i];
				if (opt.selected) {
					var nextOpt = selectOptions[selectOptions.length - 1];
					opt = selected_tabs.removeChild(opt);
					nextOpt = selected_tabs.replaceChild(opt, nextOpt);
					selected_tabs.insertBefore(nextOpt, opt);
				}
			}
		}

		function move_up(selected_tabs) {
			var selected_tabs = document.getElementById(selected_tabs);
			var selectOptions = selected_tabs.getElementsByTagName('option');
			for (var i = 1; i < selectOptions.length; i++) {
				var opt = selectOptions[i];
				if (opt.selected) {
					selected_tabs.removeChild(opt);
					selected_tabs.insertBefore(opt, selectOptions[i - 1]);
				}
			}
		}

      const xValues = ["Total Calls", "Answered Calls", "Unanswered Calls"];
      const yValues = [55, 49, 44];
      const barColors = [
        "#f4783b",
        "#3cbb96",
        "#6259ca"
      ];
      
      new Chart("myChart7", {
        type: "line",
        data: {
          labels: xValues,
          datasets: [{
            backgroundColor: barColors,
            data: yValues
          }]
        },
        options: {
          title: {
            display: false,
            text: "World Wide Wine Production 2018"
          }
        }
      });

      new Chart("myChart8", {
