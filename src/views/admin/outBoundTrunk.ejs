    <script>
      const xValues = ["Total Calls", "Answered Calls", "Unanswered Calls"];
      const yValues = [55, 49, 44];
      const barColors = [
        "#f4783b",
        "#3cbb96",
        "#6259ca"
      ];
      
      new Chart("myChart7", {
        type: "line",
        data: {
          labels: xValues,
          datasets: [{
            backgroundColor: barColors,
            data: yValues
          }]
        },
        options: {
          title: {
            display: false,
            text: "World Wide Wine Production 2018"
          }
        }
      });

      new Chart("myChart8", {
        type: "line",
        data: {
          labels: xValues,
          datasets: [{
            backgroundColor: barColors,
            data: yValues
          }]
        },
        options: {
          title: {
            display: false,
            text: "World Wide Wine Production 2018"
          }
        }
      });

      new Chart("myChart9", {
        type: "line",
        data: {
          labels: xValues,
          datasets: [{
            backgroundColor: barColors,
            data: yValues
          }]
        },
        options: {
          title: {
            display: false,
            text: "World Wide Wine Production 2018"
          }
        }
      });
      function changeType(element) {
        if(element.value=="VOIP"){
          $("#otdahdi").hide();
          $("#ottechnology").hide();
          $("#otipadd").hide();
          $("#otaccdetails").show();
          $("#voiptype").show();
        }
      }

      function handleVoip(element){
        if(element.checked)
        {
          $("#voipTextBox").show();
          $("#voipshow").hide();
        }
        else{
          $("#voipTextBox").hide();
          $("#voipshow").show();

        }
      }
      function changeTypeEdit(element,index) {
        
        if(element.value=="VOIP"){
          $(`#otdahdiedit${index}`).hide();
          $(`#ottechnologyedit${index}`).hide();
          $(`#otipaddedit${index}`).hide();
          $(`#otaccdetailsedit${index}`).show();
        }
      }

      function handleVoipEdit(element,index){
        if(element.checked)
        {
          $(`#voipTextBoxedit${index}`).show();
          $(`#voipshowedit${index}`).hide();
        }
        else{
          $(`#voipTextBoxedit${index}`).hide();
          $(`#voipshowedit${index}`).show();

        }
      }
      </script>

